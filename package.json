{
  "name": "chartmetric-takehome",
  "version": "0.0.1a0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "format": "prettier --write './(*.html|*.js|*.json|src/**/*.ts|src/**/*.tsx)'",
    "test": "mocha",
    "test-one": "mocha $MATCH",
    "test-watch": "tsc-watch -p tsconfig.json --onSuccess \"npm test\"",
    "coverage": "nyc npm run test",
    "prebuild": "npm run format && npm run coverage",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "tsc-watch -p tsconfig.json --noClear --onSuccess \"node .\""
  },
  "nyc": {
    "all": true,
    "reporter": [
      "text"
    ],
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/server.ts",
      "**/dist/**",
      "**/*.spec.ts"
    ]
  },
  "mocha": {
    "extension": [
      "ts",
      "tsx"
    ],
    "spec": [
      "src/**/*.spec.ts"
    ],
    "require": "ts-node/register"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "ts-node": "^8.9.1",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
